{
    "name": "sinsquare/phpdoc-formatter",
    "description": "A tool to automatically fix phpDocumentor tags and annotations",
    "license": "MIT",
    "type": "application",
    "require": {
        "php": "^5.6 || >=7.0",
        "symfony/finder": ">=4.0",
        "symfony/polyfill-php70": ">=1.0",
        "symfony/polyfill-php72": ">=1.4",
        "symfony/stopwatch": ">=4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0",
        "friendsofphp/php-cs-fixer": ">=2.0"
    },
    "autoload": {
        "psr-4": { "PhpDocFormatter\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "PhpDocFormatter\\Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "bin": [
        "php-doc-formatter"
    ]
}
